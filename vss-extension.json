			"addressable": true
		},
		{
			"path": "static/webfonts/fa-solid-900.woff2",
			"addressable": true
		},
		{
			"path": "static/webfonts/fa-solid-900.eot",
			"addressable": true
		},
		{
			"path": "static/images/loading.gif",
			"addressable": true
		},
		{
			"path": "static/images/filter-icon-16.png",
			"addressable": true
		}		
	]
}